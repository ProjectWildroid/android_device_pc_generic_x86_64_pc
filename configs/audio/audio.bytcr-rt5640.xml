<audiohal>
    <mixer card="0">
        <init>
            <!-- codecs/rt5640/EnableSeq.conf -->
            <ctl name="DAC MIXL INF1 Switch" val="1"/>
            <ctl name="DAC MIXR INF1 Switch" val="1"/>
            <ctl name="Stereo DAC MIXL DAC L1 Switch" val="1"/>
            <ctl name="Stereo DAC MIXR DAC R1 Switch" val="1"/>
            <ctl name="Stereo DAC MIXL DAC L2 Switch" val="1"/>
            <ctl name="Stereo DAC MIXR DAC R2 Switch" val="1"/>
            <ctl name="OUT MIXL DAC L1 Switch" val="1"/>
            <ctl name="OUT MIXR DAC R1 Switch" val="1"/>
            <ctl name="SPK MIXL DAC L1 Switch" val="1"/>
            <ctl name="SPK MIXR DAC R1 Switch" val="1"/>

            <ctl name="SDI select" val="0"/>
            <ctl name="DAI select" val="0"/>

            <ctl name="DAC2 Playback Switch" val="1"/>

            <ctl name="Stereo ADC1 Mux" val="ADC"/>
            <ctl name="ADC Capture Switch" val="1"/>

            <ctl name="Mono ADC L1 Mux" val="ADCL"/>
            <ctl name="Mono ADC R1 Mux" val="ADCR"/>
            <ctl name="Mono ADC L2 Mux" val="DMIC L1"/>
            <ctl name="Mono ADC R2 Mux" val="DMIC R1"/>
            <ctl name="Mono ADC Capture Switch" val="1"/>

            <ctl name="ADC Capture Volume" val="71"/>
            <ctl name="Mono ADC Capture Volume" val="71"/>

            <ctl name="ADC Boost Gain" val="0"/>

            <ctl name="IN1 Boost" val="8"/>
            <ctl name="IN2 Boost" val="1"/>
            <ctl name="IN3 Boost" val="8"/>

            <ctl name="RECMIXL BST1 Switch" val="0"/>
            <ctl name="RECMIXR BST1 Switch" val="0"/>
            <ctl name="RECMIXL BST2 Switch" val="0"/>
            <ctl name="RECMIXR BST2 Switch" val="0"/>
            <ctl name="RECMIXL BST3 Switch" val="0"/>
            <ctl name="RECMIXR BST3 Switch" val="0"/>

            <ctl name="Stereo ADC MIXL ADC1 Switch" val="0"/>
            <ctl name="Stereo ADC MIXR ADC1 Switch" val="0"/>
            <ctl name="Stereo ADC MIXL ADC2 Switch" val="0"/>
            <ctl name="Stereo ADC MIXR ADC2 Switch" val="0"/>
            <ctl name="Mono ADC MIXL ADC1 Switch" val="0"/>
            <ctl name="Mono ADC MIXR ADC1 Switch" val="0"/>
            <ctl name="Mono ADC MIXL ADC2 Switch" val="0"/>
            <ctl name="Mono ADC MIXR ADC2 Switch" val="0"/>

            <ctl name="HP L Playback Switch" val="0"/>
            <ctl name="HP R Playback Switch" val="0"/>
            <ctl name="Speaker L Playback Switch" val="0"/>
            <ctl name="Speaker R Playback Switch" val="0"/>

            <ctl name="HP Playback Volume" val="29"/>
            <ctl name="Speaker Playback Volume" val="35"/>

            <!-- Intel/bytcr-rt5640/HiFi.conf -->
            <ctl name="Speaker Switch" val="0"/>
            <ctl name="Headphone Switch" val="0"/>
            <ctl name="Headset Mic Switch" val="0"/>
            <ctl name="Internal Mic Switch" val="0"/>
        </init>
    </mixer>
    <device name="global">
        <path name="on">
            <!-- platforms/bytcr/PlatformEnableSeq.conf -->
            <ctl name="media0_in Gain 0 Switch" val="1"/>
            <ctl name="media0_in Gain 0 Volume" val="0"/>

            <ctl name="media1_in Gain 0 Switch" val="1"/>
            <ctl name="media1_in Gain 0 Volume" val="0"/>

            <ctl name="media2_in Gain 0 Switch" val="0"/>
            <ctl name="media2_in Gain 0 Volume" val="0%"/>

            <ctl name="media3_in Gain 0 Switch" val="1"/>
            <ctl name="media3_in Gain 0 Volume" val="0"/>

            <ctl name="media0_out mix 0 media0_in Switch" val="1"/>
            <ctl name="media0_out mix 0 media1_in Switch" val="1"/>
            <ctl name="media0_out mix 0 media2_in Switch" val="0"/>
            <ctl name="media0_out mix 0 media3_in Switch" val="1"/>

            <ctl name="media1_out mix 0 media0_in Switch" val="0"/>
            <ctl name="media1_out mix 0 media1_in Switch" val="0"/>
            <ctl name="media1_out mix 0 media2_in Switch" val="0"/>
            <ctl name="media1_out mix 0 media3_in Switch" val="0"/>

            <ctl name="pcm0_in Gain 0 Switch" val="1"/>
            <ctl name="pcm0_in Gain 0 Volume" val="0"/>

            <ctl name="pcm1_in Gain 0 Switch" val="0"/>
            <ctl name="pcm1_in Gain 0 Volume" val="0%"/>

            <ctl name="codec_out0 mix 0 codec_in0 Switch" val="0"/>
            <ctl name="codec_out0 mix 0 codec_in1 Switch" val="0"/>
            <ctl name="codec_out0 mix 0 media_loop1_in Switch" val="0"/>
            <ctl name="codec_out0 mix 0 media_loop2_in Switch" val="0"/>
            <ctl name="codec_out0 mix 0 pcm0_in Switch" val="1"/>
            <ctl name="codec_out0 mix 0 pcm1_in Switch" val="0"/>
            <ctl name="codec_out0 mix 0 sprot_loop_in Switch" val="0"/>
            <ctl name="codec_out0 Gain 0 Switch" val="1"/>
            <ctl name="codec_out0 Gain 0 Volume" val="0"/>

            <ctl name="modem_out mix 0 codec_in0 Switch" val="0"/>
            <ctl name="modem_out mix 0 codec_in1 Switch" val="0"/>
            <ctl name="modem_out mix 0 media_loop1_in Switch" val="0"/>
            <ctl name="modem_out mix 0 media_loop2_in Switch" val="0"/>
            <ctl name="modem_out mix 0 pcm0_in Switch" val="1"/>
            <ctl name="modem_out mix 0 pcm1_in Switch" val="0"/>
            <ctl name="modem_out mix 0 sprot_loop_in Switch" val="0"/>
            <ctl name="modem_out Gain 0 Switch" val="1"/>
            <ctl name="modem_out Gain 0 Volume" val="0"/>

            <ctl name="codec_in0 Gain 0 Switch" val="1"/>
            <ctl name="codec_in0 Gain 0 Volume" val="0"/>

            <ctl name="modem_in Gain 0 Switch" val="1"/>
            <ctl name="modem_in Gain 0 Volume" val="0"/>

            <ctl name="pcm1_out mix 0 codec_in0 Switch" val="1"/>
            <ctl name="pcm1_out mix 0 modem_in Switch" val="1"/>
            <ctl name="pcm1_out mix 0 codec_in1 Switch" val="0"/>
            <ctl name="pcm1_out mix 0 media_loop1_in Switch" val="0"/>
            <ctl name="pcm1_out mix 0 media_loop2_in Switch" val="0"/>
            <ctl name="pcm1_out mix 0 pcm0_in Switch" val="0"/>
            <ctl name="pcm1_out mix 0 pcm1_in Switch" val="0"/>
            <ctl name="pcm1_out mix 0 sprot_loop_in Switch" val="0"/>

            <ctl name="pcm1_out Gain 0 Switch" val="1"/>
            <ctl name="pcm1_out Gain 0 Volume" val="0"/>

            <ctl name="codec_out1 mix 0 codec_in0 Switch" val="0"/>
            <ctl name="codec_out1 mix 0 codec_in1 Switch" val="0"/>
            <ctl name="codec_out1 mix 0 media_loop1_in Switch" val="0"/>
            <ctl name="codec_out1 mix 0 media_loop2_in Switch" val="0"/>
            <ctl name="codec_out1 mix 0 pcm0_in Switch" val="0"/>
            <ctl name="codec_out1 mix 0 pcm1_in Switch" val="0"/>
            <ctl name="codec_out1 mix 0 sprot_loop_in Switch" val="0"/>
            <ctl name="codec_out1 Gain 0 Switch" val="0"/>
            <ctl name="codec_out1 Gain 0 Volume" val="0%"/>

            <ctl name="codec_in1 Gain 0 Switch" val="0"/>
            <ctl name="codec_in1 Gain 0 Volume" val="0%"/>

            <ctl name="media_loop1_out mix 0 codec_in0 Switch" val="0"/>
            <ctl name="media_loop1_out mix 0 codec_in1 Switch" val="0"/>
            <ctl name="media_loop1_out mix 0 media_loop1_in Switch" val="0"/>
            <ctl name="media_loop1_out mix 0 media_loop2_in Switch" val="0"/>
            <ctl name="media_loop1_out mix 0 pcm0_in Switch" val="0"/>
            <ctl name="media_loop1_out mix 0 pcm1_in Switch" val="0"/>
            <ctl name="media_loop1_out mix 0 sprot_loop_in Switch" val="0"/>

            <ctl name="media_loop2_out mix 0 codec_in0 Switch" val="0"/>
            <ctl name="media_loop2_out mix 0 codec_in1 Switch" val="0"/>
            <ctl name="media_loop2_out mix 0 media_loop1_in Switch" val="0"/>
            <ctl name="media_loop2_out mix 0 media_loop2_in Switch" val="0"/>
            <ctl name="media_loop2_out mix 0 pcm0_in Switch" val="0"/>
            <ctl name="media_loop2_out mix 0 pcm1_in Switch" val="0"/>
            <ctl name="media_loop2_out mix 0 sprot_loop_in Switch" val="0"/>

            <ctl name="sprot_loop_out mix 0 codec_in0 Switch" val="0"/>
            <ctl name="sprot_loop_out mix 0 codec_in1 Switch" val="0"/>
            <ctl name="sprot_loop_out mix 0 media_loop1_in Switch" val="0"/>
            <ctl name="sprot_loop_out mix 0 media_loop2_in Switch" val="0"/>
            <ctl name="sprot_loop_out mix 0 pcm0_in Switch" val="0"/>
            <ctl name="sprot_loop_out mix 0 pcm1_in Switch" val="0"/>
            <ctl name="sprot_loop_out mix 0 sprot_loop_in Switch" val="0"/>
        </path>
        <path name="off">
            <!-- platforms/bytcr/PlatformDisableSeq.conf -->
            <ctl name="codec_out0 mix 0 pcm0_in Switch" val="0"/>
            <ctl name="modem_out mix 0 pcm0_in Switch" val="0"/>

            <ctl name="media0_out mix 0 media0_in Switch" val="0"/>
            <ctl name="media0_out mix 0 media1_in Switch" val="0"/>
            <ctl name="media0_out mix 0 media3_in Switch" val="0"/>

            <ctl name="media0_in Gain 0 Switch" val="0"/>
            <ctl name="media1_in Gain 0 Switch" val="0"/>
            <ctl name="media3_in Gain 0 Switch" val="0"/>

            <ctl name="pcm0_in Gain 0 Switch" val="0"/>
            <ctl name="codec_out0 Gain 0 Switch" val="0"/>
            <ctl name="modem_out Gain 0 Switch" val="0"/>

            <ctl name="pcm1_out Gain 0 Switch" val="0"/>
            <ctl name="codec_in0 Gain 0 Switch" val="0"/>
            <ctl name="modem_in Gain 0 Switch" val="0"/>
        </path>
    </device>
    <device name="headphone">
        <path name="on">
            <!-- codecs/rt5640/HeadPhones.conf -->
            <ctl name="HPO MIX HPVOL Switch" val="1"/>
            <ctl name="HP Channel Switch" val="1"/>
            <ctl name="HP L Playback Switch" val="1"/>
            <ctl name="HP R Playback Switch" val="1"/>
            <!-- Intel/bytcr-rt5640/HiFi.conf -->
            <ctl name="Headphone Switch" val="1"/>
        </path>
        <path name="off">
            <!-- codecs/rt5640/HeadPhones.conf -->
            <ctl name="HP Channel Switch" val="0"/>
            <ctl name="HP L Playback Switch" val="0"/>
            <ctl name="HP R Playback Switch" val="0"/>
            <!-- Intel/bytcr-rt5640/HiFi.conf -->
            <ctl name="Headphone Switch" val="0"/>
        </path>
    </device>
    <device name="headset">
        <path name="on">
            <!-- codecs/rt5640/HeadPhones.conf -->
            <ctl name="HPO MIX HPVOL Switch" val="1"/>
            <ctl name="HP Channel Switch" val="1"/>
            <ctl name="HP L Playback Switch" val="1"/>
            <ctl name="HP R Playback Switch" val="1"/>
            <ctl name="Headphone Switch" val="1"/>
        </path>
        <path name="off">
            <!-- codecs/rt5640/HeadPhones.conf -->
            <ctl name="HP Channel Switch" val="0"/>
            <ctl name="HP L Playback Switch" val="0"/>
            <ctl name="HP R Playback Switch" val="0"/>
            <!-- Intel/bytcr-rt5640/HiFi.conf -->
            <ctl name="Headphone Switch" val="0"/>
        </path>
    </device>
    <device name="headset_in">
        <path name="on">
            <!-- codecs/rt5640/HeadsetMic*.conf -->
            <ctl name="RECMIXL BST1 Switch" val="1"/>
            <ctl name="RECMIXR BST1 Switch" val="1"/>
            <ctl name="RECMIXL BST2 Switch" val="1"/>
            <ctl name="RECMIXR BST2 Switch" val="1"/>

            <ctl name="Stereo ADC MIXL ADC1 Switch" val="1"/>
            <ctl name="Stereo ADC MIXR ADC1 Switch" val="1"/>
            <ctl name="Mono ADC MIXL ADC1 Switch" val="1"/>
            <ctl name="Mono ADC MIXR ADC1 Switch" val="1"/>
            <!-- Intel/bytcr-rt5640/HiFi.conf -->
            <ctl name="Headset Mic Switch" val="1"/>
            <ctl name="Headset Mic 2 Switch" val="1"/>
        </path>
        <path name="off">
            <!-- codecs/rt5640/HeadsetMic*.conf -->
            <ctl name="Stereo ADC MIXL ADC1 Switch" val="0"/>
            <ctl name="Stereo ADC MIXR ADC1 Switch" val="0"/>
            <ctl name="Mono ADC MIXL ADC1 Switch" val="0"/>
            <ctl name="Mono ADC MIXR ADC1 Switch" val="0"/>

            <ctl name="RECMIXL BST2 Switch" val="0"/>
            <ctl name="RECMIXR BST2 Switch" val="0"/>
            <ctl name="RECMIXL BST1 Switch" val="0"/>
            <ctl name="RECMIXR BST1 Switch" val="0"/>
            <!-- Intel/bytcr-rt5640/HiFi.conf -->
            <ctl name="Headset Mic Switch" val="0"/>
            <ctl name="Headset Mic 2 Switch" val="0"/>
        </path>
    </device>
    <device name="mic">
        <path name="on">
            <!-- codecs/rt5640/IN?-InternalMic.conf -->
            <ctl name="RECMIXL BST1 Switch" val="1"/>
            <ctl name="RECMIXR BST1 Switch" val="1"/>
            <ctl name="RECMIXL BST3 Switch" val="1"/>
            <ctl name="RECMIXR BST3 Switch" val="1"/>

            <ctl name="Stereo ADC MIXL ADC1 Switch" val="1"/>
            <ctl name="Stereo ADC MIXR ADC1 Switch" val="1"/>
            <ctl name="Mono ADC MIXL ADC1 Switch" val="1"/>
            <ctl name="Mono ADC MIXR ADC1 Switch" val="1"/>
            <!-- Intel/bytcr-rt5640/HiFi.conf -->
            <ctl name="Internal Mic Switch" val="1"/>
        </path>
        <path name="off">
            <!-- codecs/rt5640/IN?-InternalMic.conf -->
            <ctl name="Stereo ADC MIXL ADC1 Switch" val="0"/>
            <ctl name="Stereo ADC MIXR ADC1 Switch" val="0"/>
            <ctl name="Mono ADC MIXL ADC1 Switch" val="0"/>
            <ctl name="Mono ADC MIXR ADC1 Switch" val="0"/>

            <ctl name="RECMIXL BST3 Switch" val="0"/>
            <ctl name="RECMIXR BST3 Switch" val="0"/>
            <ctl name="RECMIXL BST1 Switch" val="0"/>
            <ctl name="RECMIXR BST1 Switch" val="0"/>
            <!-- Intel/bytcr-rt5640/HiFi.conf -->
            <ctl name="Internal Mic Switch" val="0"/>
        </path>
    </device>
    <device name="speaker">
        <path name="on">
            <!-- codecs/rt5640/Speaker.conf -->
            <ctl name="SPOL MIX SPKVOL L Switch" val="1"/>
            <ctl name="SPOR MIX SPKVOL R Switch" val="1"/>

            <ctl name="SPOL MIX SPKVOL R Switch" val="0"/>
            <ctl name="Speaker Channel Switch" val="1"/>
            <ctl name="Speaker L Playback Switch" val="1"/>
            <ctl name="Speaker R Playback Switch" val="1"/>
            <!-- Intel/bytcr-rt5640/HiFi.conf -->
            <ctl name="Speaker Switch" val="1"/>
        </path>
        <path name="off">
            <!-- codecs/rt5640/Speaker.conf -->
            <ctl name="Speaker Channel Switch" val="0"/>
            <ctl name="Speaker L Playback Switch" val="0"/>
            <ctl name="Speaker R Playback Switch" val="0"/>
            <!-- Intel/bytcr-rt5640/HiFi.conf -->
            <ctl name="Speaker Switch" val="0"/>
        </path>
    </device>
    <stream type="pcm" dir="in" card="0" device="0">
    </stream>
    <stream type="pcm" dir="out" card="0" device="0">
    </stream>
</audiohal>
