# ACPI
CONFIG_ACPI_AC=y
CONFIG_ACPI_BATTERY=y
CONFIG_ACPI_FAN=y
CONFIG_ACPI_THERMAL=y

# ATA
CONFIG_ATA=y
CONFIG_ATA_GENERIC=y
CONFIG_ATA_PIIX=y
CONFIG_PATA_ACPI=y
CONFIG_SATA_AHCI=y
CONFIG_SATA_AHCI_PLATFORM=y

# Block device
CONFIG_BLK_DEV_SR=y

# CPU freq
CONFIG_X86_ACPI_CPUFREQ=y

# DMA BUF
CONFIG_DMABUF_HEAPS_SYSTEM=y

# DRM
CONFIG_DRM_FBDEV_EMULATION=y

# EFI
CONFIG_EFI_STUB=y

# Filesystem
CONFIG_EFIVAR_FS=y
CONFIG_ISO9660_FS=y
CONFIG_NTFS_FS=y

# Framebuffer
CONFIG_FB=y
CONFIG_FB_EFI=y
CONFIG_FB_VESA=y

# Framebuffer console
CONFIG_FRAMEBUFFER_CONSOLE=y

# Mouse
CONFIG_MOUSE_PS2=y

# Processor
CONFIG_NUMA=y

# USB
CONFIG_USB_UHCI_HCD=y
CONFIG_USB_OHCI_HCD=y

# Virtual Terminal
CONFIG_VT=y
CONFIG_VT_HW_CONSOLE_BINDING=y
